<!DOCTYPE html>
<html>
    <head>
        {{with .SiteTitle}}<title>{{.}}</title>{{end}}
        {{with .CSS}}<link href="{{.}}" rel="stylesheet">{{end}}
    </head>
    <body>
        <header>
        {{with .Logo}}<a href="/">{{.}}</a>{{end}}
        {{with .SiteTitle}}<h1>{{.}}</h1>{{end}}
        </header>
        {{with .Nav }}<nav>{{.}}</nav>{{end}}
        <section>
        {{with .SiteSlogan}}<h2>{{.}}</h2>{{end}}
        {{with .SiteDescription}}{{.}}{{end}}
        <p>
        {{with .ArticleList}}{{.}}{{end}}
        </section>
        {{with .SideBar}}<aside>{{.}}</aside>{{end}}
        <footer>
            {{.SiteInfo}} 
            {{.SiteCopyright}}
        </footer>
    </body>
</html>
